<?xml version="1.0" encoding="utf-8" ?>
<theme xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
    xs:noNamespaceSchemaLocation="http://www.csphere.eu/xml/schemas/theme.xsd">
    <vendor>acme</vendor>
    <version>0.1</version>
    <published>2007-04-07</published>
    <copyright>cSphere Team</copyright>
    <license>Simplified BSD License</license>
    <name>Default</name>
    <info>This template is shipped with the standard package</info>
    <engine name="csphere" version_min="2012.0" />
    <icon type="default">layout</icon>
    <contains admin="false" install="false" layout="true" mobile="false" />
    <authors>
        <current>cSphere Team</current>
        <past>cSphere Team</past>
    </authors>
    <contact>
        <email adress="contact@csphere.eu" />
        <web url="http://www.csphere.eu" />
    </contact>
    <media>
        <preview>images/preview.png</preview>
        <thumb>images/thumb.png</thumb>
    </media>
    <environment>
        <needed vendor="csphere" plugin="groups" version_min="0.1" />
        <needed vendor="csphere" plugin="news" version_min="0.1" />
    </environment>
</theme>
